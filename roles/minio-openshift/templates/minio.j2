---
apiVersion: apps.openshift.io/v1
kind: DeploymentConfig
metadata:
  labels:
    app: minio
  name: minio
spec:
  replicas: 1
  selector:
    app: minio
    deploymentconfig: minio
  template:
    metadata:
      labels:
        app: minio
        deploymentconfig: minio
    spec:
      volumes:
        - name: secret-volume
          secret:
            secretName: {{ tsl_secret_name }}
            items:
            - key:  tls.crt
              path: public.crt
            - key: tls.key
              path: private.key
            - key: tls.crt
              path: CAs/public.crt
      containers:
        - env:
            - name: MINIO_ROOT_USER
              value: {{minio_root_user}}
            - name: MINIO_ROOT_PASSWORD
              value: {{minio_root_password}}
          image: >-
            quay.io/minio/minio
          name: minio
          args:
            - server
            - /data
            - --console-address
            - ":9001"
          volumeMounts:
            - name: secret-volume
              mountPath: /root/.minio/certs
---
apiVersion: v1
kind: Service
metadata:
  labels:
    app: minio
  name: minio
spec:
  ports:
    - name: 9000-tcp
      port: 9000
      protocol: TCP
      targetPort: 9000
    - name: 9001-http
      port: 9001
      protocol: TCP
      targetPort: 9001

  selector:
    app: minio
    deploymentconfig: minio
